<?xml version="1.0" encoding="UTF-8"?>
<plugin title="portalboilerplate" loader="DEFAULT">
<overlays>
		<overlay id="startup.portalboilerplate" resourcebundle="content/portalboilerplate/resources/lang/messages">
			<menubar id="toolsmenu">
	        	<menuitem id="portalboilerplate" label="${Launcher.portalboilerplate}" command="mantleXulHandler.openUrl('${Launcher.portalboilerplate}','${Launcher.portalboilerplate_TOOLTIP}','plugin/portalboilerplate/api/default')"/>
	      	</menubar>
	    </overlay>
	</overlays>
	<lifecycle-listener class="pt.webdetails.cpk.CpkLifecycleListener"/>
	<static-paths>
		<static-path url="/portalboilerplate/static" localFolder="static"/>
		<static-path url="/portalboilerplate/resources" localFolder="resources"/>
	</static-paths>
	<content-types>
		<content-type>
			<title>Portalboilerplate</title>
			<description></description>
			<company name="" url="" logo="@PLUGIN_COMPANY_LOGO@"></company>
		</content-type>
	</content-types>
	<content-generator
		id="portalboilerplate"
		title="Portalboilerplate"
		type="portalboilerplate"
		class="pt.webdetails.cpk.CpkContentGenerator"/>
	<!-- Menu entry -->
	<menu-items>
		<!--<menu-item id="portalboilerplate_main" anchor="tools-submenu" label="Portalboilerplate" command="content/portalboilerplate/" type="MENU_ITEM" how="LAST_CHILD"/>-->
	</menu-items>
</plugin>
